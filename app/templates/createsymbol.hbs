{{input value=symbol class='createsymbol__inputsymbol' placeholder='new symbol' type="text"}}
<span>Диалект:</span>
<select class="createsymbol__select_dialects" onchange={{action 'getdialect' value="target.value"}}>
  {{#each select_dialects as |dialect|}}
    <option>{{dialect.dialect}}</option>
  {{/each}}
</select>
<span>Описание:</span>
<textarea onchange={{action 'getdescription' value='target.value'}}></textarea>
<input class='createsymbol__inputfile' type="file" onchange={{action 'addfile' }}>
<button onclick={{action 'createsymbol' }}>Создать</button>